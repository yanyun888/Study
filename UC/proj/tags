_STUSYS_H	stuSys.h	8;"	d
addData	stuSys.c	/^int addData(int fd,STU* pSTU){$/;"	f
delData	stuSys.c	/^int delData(int fd,int id){$/;"	f
getData	stuSys.c	/^int getData(int fd,int id){$/;"	f
getStu	stuSys.c	/^int getStu(STU* pSTU){$/;"	f
id	stuSys.h	/^    int id;$/;"	m	struct:STU
name	stuSys.h	/^    char name[20];$/;"	m	struct:STU
